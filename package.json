{
  "name": "randos-note",
  "displayName": "Randos Note",
  "description": "Open and jot down thoughts in a single rolling notes file from the Command Palette.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.1"
  },
    "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:HakeemIshak/randos-note.git"
  },
  "license": "CC0-1.0",
  "homepage": "https://github.com/HakeemIshak/randos-note#readme",
  "categories": ["Notebooks"],
  "keywords": ["notes", "scratchpad", "quick note", "randos"],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "randosNote.open",
        "title": "Randos Note: Open File"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Randos Note",
      "properties": {
        "randosNote.filePath": {
          "type": "string",
          "default": "Documents/randos.txt",
          "description": "Absolute path to the single randos-note file"
        }
      }
    },
    "keybindings": [
      {
        "command": "randosNote.open",
        "key": "ctrl+alt+r",
        "mac": "cmd+alt+r",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
